{
  "name": "vda",
  "productName": "Visual Database Assurer",
  "version": "1.2.0",
  "description": "Excel reading and writing app using Electron",
  "main": "main.js",
  "scripts": {
    "test": "mocha tests/",
    "babel": "babel",
    "webpack": "webpack --watch",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=../dist/win/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Visual Database Assurer\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Fenway Group",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-jquery": "^2.0.0",
    "electron": "^1.7.9",
    "electron-packager": "^8.7.2",
    "mocha": "^3.5.0",
    "redux-devtools": "^3.4.0",
    "spectron": "^3.7.2",
    "webpack": "^2.7.0"
  },
