    "react": "^15.6.1",
    "react-checkbox": "^0.1.3",
    "react-dom": "^15.6.1",
    "react-form": "^1.3.0",
    "react-redux": "^5.0.6",
    "react-simple-dropdown": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
